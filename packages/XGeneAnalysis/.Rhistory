create_od_list <- function(genotypes_tested,file_correspondence_list){
file_correspondence_list <- read.csv(file_correspondence_list,head=T,sep='\t',stringsAsFactors=F)
experiment_position_map <- create_experiment_position_map(file_correspondence_list)
od_list <- read_tecan_files_to_list(file_correspondence_list[,2])
geno_combos <- create_all_genotype_names(genotypes_tested)
drug_concentrations <- names(experiment_position_map[[1]])
od_results <- list()
for(i in 1:length(geno_combos)){
for(j in 1:length(drug_concentrations)){
refmap <- experiment_position_map[[geno_combos[i]]][[drug_concentrations[j]]]
fluconazole_wells <- refmap$fluconazole$ref_well
dmso_well <- refmap$dmso$ref_well
od_file <- od_list[[refmap$filename]]
od_results[[geno_combos[i]]][[drug_concentrations[j]]]$y <- apply(od_file[,fluconazole_wells],1,mean)
od_results[[geno_combos[i]]][[drug_concentrations[j]]]$x <- od_file[,'RunT.s.']
od_results[[geno_combos[i]]][[drug_concentrations[j]]]$auc_ratio <- sapply(fluconazole_wells,function(fluconazole_well){calculate_auc(od_file,fluconazole_well)/calculate_auc(od_file,dmso_well)})
}
}
return(od_results)
}
